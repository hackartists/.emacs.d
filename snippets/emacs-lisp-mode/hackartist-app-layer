# name: hackaritst app layer
# key: hackartst app
# --

;; ${1:package name} requires below layers of spacemacs.
(setq hackartist-$1-layers '())

;; $1 requires additional packages.
(setq hackartist-$1-packages '())

;; $1 requires open source code from github
(setq hackartist-$1-osc '())

(defun hackartist/$1/init ()
"initialization code"
    $0
)

(defun hackartist/$1/config ()
"configuration code")

(defun hackartist/$1/bindings ()
"configuration code")
