    responses:
      "200":
        content:
          application/json:
            schema:
              $ref: "#/components/schemas/${1:Model}"
        description: >-
          ${2:response description}
      "400":
        $ref: "#/components/responses/BadRequest"
      "403":
        $ref: "#/components/responses/Forbidden"