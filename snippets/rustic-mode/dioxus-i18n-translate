# -*- mode: snippet -*-
#name : dioxus-i18n-translate
#key : dioxus-i18n-translate
#contributor : hackartist
# --

pub struct PagesTranslate<'a> {
    pub text: &'a str,
}

pub fn translate_pages<'a>(lang: &'a Language) -> PagesTranslate<'a> {
    if lang.clone() == Language::En {
        PagesTranslate {
            text: "homepage page",
        }
    } else {
        PagesTranslate {
            text: "homepage page",
        }
    }
}