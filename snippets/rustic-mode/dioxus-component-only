#[component]
pub fn ${1:ComponentName}(
    #[props(default ="${1:$(string-inflection-underscore-function yas-text)}".to_string())] id: String,
    #[props(default ="".to_string())] class: String,
) -> Element {
    rsx! {
      div { id, class, "$1"}
    }
}

