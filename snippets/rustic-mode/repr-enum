    serde_repr::Serialize_repr,
    serde_repr::Deserialize_repr,
    Default,
    DynamoEnum,
    schemars::JsonSchema_repr,

)]
#[repr(u8)]
pub enum ${1:EnumName} {
    #[default]
    ${2:VarName} = 1,
}
