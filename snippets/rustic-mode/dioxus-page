#![allow(non_snake_case)]
use dioxus::prelude::*;
use dioxus_translate::*;
use super::i18n::*;

#[component]
pub fn ${1:$(string-inflection-upper-camelcase-function (file-name-nondirectory (directory-file-name (file-name-directory buffer-file-name))))}Page(
    lang: Language,
) -> Element {
let tr: `(string-inflection-upper-camelcase-function (file-name-nondirectory (directory-file-name (file-name-directory buffer-file-name))))`Translate = translate(&lang);

    rsx! {
    div {id: "`(string-inflection-kebab-case-function (file-name-nondirectory (directory-file-name (file-name-directory buffer-file-name))))`", "$1 page"}
    }
}

