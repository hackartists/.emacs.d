#![allow(non_snake_case)]
use dioxus::prelude::*;
#![allow(unused_imports)]
use crate::prelude::*;

#[component]
pub(super) fn ${1:ComponentName}() -> Element {
    let ctrl = super::controller::Controller::use_controller();

    rsx! {
      div { "$1 section"}
    }
}

