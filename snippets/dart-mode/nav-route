# name: nav-route-push
# key: navr
# --

Navigator.of(context).push(
MaterialPageRoute(
builder: (context) {
return Scaffold(
appBar: AppBar(
title: Text(${1:title}),
),
floatingActionButton: FloatingActionButton(
tooltip: ${2:label},
onPressed: () {},
child: Icon(Icons.search),
),
body: Container(),
);
},
),
);